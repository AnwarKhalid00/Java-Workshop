package Day21;
class Item{
    int o;
    boolean produced=false;
    public void synchronized produced(int x){
        if(produced){
            try{
                wait();
            }catch(Exception e){

            }
            i = x;
            produced = true;
            notify();
        }
        consume(){
            if(!produced){
                try {
                    wait();
                } catch (Exception e) {
                    
                }
                produced = false;
                notify();
            }
        }
    }
}
